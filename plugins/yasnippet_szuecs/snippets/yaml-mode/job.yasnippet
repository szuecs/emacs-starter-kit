# name: Kubernetes job
# key: job
# --
apiVersion: batch/v1
kind: Job
metadata:
  name: ${1:name}
  namespace: ${2:ns}
spec:
  template:
    metadata:
      name: ${1:name}
    spec:
      containers:
        - name: ${1:name}
          image: ${3:image:version}
          command:
            - date
            - +%s

